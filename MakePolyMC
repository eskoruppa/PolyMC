clean_release: 
	rm -f $(OBJ_RELEASE) $(OUT_RELEASE)
	rm -rf bin/Release
	rm -rf $(OBJDIR_RELEASE)/Unbound
	rm -rf $(OBJDIR_RELEASE)/Unbound/PairStyles
	rm -rf $(OBJDIR_RELEASE)/dump
	rm -rf $(OBJDIR_RELEASE)/Unbound/Interactions
	rm -rf $(OBJDIR_RELEASE)/PolyMC
	rm -rf $(OBJDIR_RELEASE)/Observables
	rm -rf $(OBJDIR_RELEASE)/PlecFinder
	rm -rf $(OBJDIR_RELEASE)/PlecFreeEnergy
	rm -rf $(OBJDIR_RELEASE)
	rm -rf $(OBJDIR_RELEASE)/ExcludedVolume
	rm -rf $(OBJDIR_RELEASE)/Electrostatics
	rm -rf $(OBJDIR_RELEASE)/Electrostatics/Potentials
	rm -rf $(OBJDIR_RELEASE)/Constraints
	rm -rf $(OBJDIR_RELEASE)/Extra
	rm -rf $(OBJDIR_RELEASE)/Input
	rm -rf $(OBJDIR_RELEASE)/BPStep
	rm -rf $(OBJDIR_RELEASE)/BPStep/CalEnergy
	rm -rf $(OBJDIR_RELEASE)/BPStep/EvalEnergy
	rm -rf $(OBJDIR_RELEASE)/Calcs
	rm -rf $(OBJDIR_RELEASE)/ChainFuncs
	rm -rf $(OBJDIR_RELEASE)/MCStep
	rm -rf $(OBJDIR_RELEASE)/MCMethods

.PHONY: before_release after_release clean_release

